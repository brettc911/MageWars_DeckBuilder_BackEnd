    "debug": "~2.6.3",
    "express": "^4.16.2",
    "hjs": "~0.0.6",
    "mongoose": "^4.12.2",
    "morgan": "~1.8.1",
    "npm": "^5.8.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "query-string": "^6.0.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "repository": {
    "type": "git",

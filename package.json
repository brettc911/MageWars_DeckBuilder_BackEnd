{
  "name": "mage-wars",
  "version": "0.0.0",
  "description": "Mage wars app",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "test": "node test.js",
    "client": "cd client && npm start",
    "server": "nodemon ./bin/www",
